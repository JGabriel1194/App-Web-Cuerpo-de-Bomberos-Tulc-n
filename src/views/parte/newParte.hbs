                                                <h5>Tipo de Local</h5>
                                            </div>
                                            <div class="d-flex justify-content-center">
                                                <div class="btn" data-toggle="buttons">
                                                    {{#each local}}
                                                    <label class="btn btn-light">
                                                        <input type="radio" name="idTLocal" id="option1"
                                                            value="{{idTLocal}}">
                                                        {{TLocal}}
                                                    </label>
                                                    {{/each}}
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="row p-2">
                                    <div class="col-md-12">
                                        <div class="card">
                                            <div class="card-footer">
                                                <button class="btn btn-primary">
                                                    Guardar
                                                </button>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
