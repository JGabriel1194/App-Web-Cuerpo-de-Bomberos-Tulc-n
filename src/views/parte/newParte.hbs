                        <div class="tab-pane fade" id="afectado" role="tabpanel" aria-labelledby="afectado-tab">
                            <div class="border container">
                                <div class="row p-2">
                                    <div class="col-md-8">
                                        <div class="card">
                                            <div class="card-header">
                                                <h5>Datos del Afectado</h5>
                                            </div>
                                            <div class="card-body">
                                                <div class="row container">
                                                    <div class="form-group col-md-12">
                                                        <label>Nombre del Local:</label>
                                                        <input class="form-control" type="text" name="NLocal"
                                                            placeholder="Ingrese Nombre De Local" autofocus>
                                                    </div>
                                                </div>
                                                <div class="row container">
                                                    <div class="form-group col-md-12">
                                                        <label>Propietario/Arrendatario:</label>
                                                        <input class="form-control" type="text" name="Propietario"
                                                            placeholder="Ingrese Propietario/Arrendatario" autofocus>
