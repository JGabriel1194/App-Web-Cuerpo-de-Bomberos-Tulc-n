                <form action="/parte/add" method="POST" enctype="multipart/form-data">
                    <div class="tab-content" id="myTabContent">
                        <!--Datos Generales -->
                        <div class="tab-pane fade show active" id="general" role="tabpanel"
                            aria-labelledby="general-tab">
                            <div class="border container">
                                <div class="row p-2">
                                    <div class="col-md-4">
                                        <div class="card">
                                            <div class="card-header">
                                                <h5>Fecha</h5>
                                            </div>
                                            <div class="d-flex justify-content-center">
                                                <div id="fecha1" class="col-xs-3 input-group">
                                                    <input data-format="yyyy-MM-dd" type="text" class="form-control"
                                                        id="datepicker" name="Fecha" autocomplete="off"
                                                        placeholder="Seleccione una fecha">
                                                    </input>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="row p-2">
                                    <div class="col-md-12">
                                        <div class="card">
                                            <div class="card-header">
                                                <h5>Tipo Emergencia</h5>
                                            </div>
                                            <div class="d-flex justify-content-center">
                                                <div class="btn" data-toggle="buttons">
                                                    {{#each emergency}}
                                                    <label class="btn btn-light">
                                                        <input type="radio" name="idTEmergencia" id="option1"
                                                            value="{{idTEmergencia}}">
                                                        {{TEmergencia}}
                                                    </label>
                                                    {{/each}}
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="row p-2">
                                    <div class="col-md-12">
                                        <div class="card">
                                            <div class="card-header">
                                                <h5>Clase de Día</h5>
                                            </div>
                                            <div class="d-flex justify-content-center">
                                                <div class="btn" data-toggle="buttons">
                                                    {{#each days}}
                                                    <label class="btn btn-light">
                                                        <input type="radio" name="idCDia" id="option1"
                                                            value="{{idCDia}}">
                                                        {{CDia}}
                                                    </label>
                                                    {{/each}}
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="row p-2">
                                    <div class="col-md-12">
                                        <div class="card">
                                            <div class="card-footer">
                                                <button class="btn btn-primary">
                                                    Guardar
                                                </button>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <!--Atendido por -->
                        <div class="tab-pane fade" id="atendido" role="tabpanel" aria-labelledby="atendido-tab">
                            <div class="border container">
                                <div class="row p-2">
                                    <div class="col-md-8">
                                        <div class="card">
                                            <div class="card-header">
                                                <h5>Compañia</h5>
                                            </div>
                                            <div class="d-flex justify-content-center">
                                                <div class="btn" data-toggle="buttons">
                                                    {{#each emergency}}
                                                    <label class="btn btn-light">
                                                        <input type="radio" name="idUnidad" id="option1"
                                                            value="{{idTEmergencia}}">
                                                        {{TEmergencia}}
                                                    </label>
                                                    {{/each}}
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-4">
                                        <div class="card">
                                            <div class="card-header">
                                                <h5>Apoyo Requerido</h5>
                                            </div>
                                            <div class="d-flex justify-content-center">
                                                <div class="btn" data-toggle="buttons">
                                                    <label class="btn btn-light">
                                                        <input type="radio" name="Apoyo" id="option1" value="Si">
                                                        Si
                                                    </label>
                                                    <label class="btn btn-light">
                                                        <input type="radio" name="Apoyo" id="option1" value="No">
                                                        No
                                                    </label>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="row p-2">
                                    <div class="col-md-6">
                                        <div class="card">
                                            <div class="card-header">
                                                <i class="fas fa-table"></i>
                                                Personal que Concurrió
                                                <a class="btn btn-success float-right" data-toggle="modal"
                                                    data-target="#new" href="#">
                                                    Nuevo
                                                </a>
                                            </div>
                                            <div class="card-body">
                                                <table class="table table-bordered" id="dataTable" width="100%"
                                                    cellspacing="0">
                                                    <thead>
                                                        <tr>
                                                            <th>N°</th>
                                                            <th>Personal</th>
                                                            <th>Tarea</th>
                                                        </tr>
                                                    </thead>
                                                    <tfoot>
                                                        <tr>
                                                            <th>N°</th>
                                                            <th>Personal</th>
                                                            <th>Tarea</th>
                                                        </tr>
                                                    </tfoot>
                                                    <tbody>
                                                        {{#each days}}
                                                        <tr>
                                                            <td style="vertical-align:middle;">{{math @index "+" 1}}
                                                            </td>
                                                            <td style="vertical-align:middle;"> {{CDia}}</td>
                                                            <td style="vertical-align:middle;">
                                                                <button type="button" class="btn btn-danger"
                                                                    data-toggle="modal" data-target="#alert"
                                                                    data-id="/start/delete/{{idCDia}}">
                                                                    <i class="fas fa-fw fa-trash-alt"></i>
                                                                </button>
                                                            </td>
                                                        </tr>
                                                        {{/each}}
                                                    </tbody>
                                                </table>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-6">
                                        <div class="card">
                                            <div class="card-header">
                                                <i class="fas fa-table"></i>
                                                Vehículos
                                                <a class="btn btn-success float-right" data-toggle="modal"
                                                    data-target="#new" href="#">
                                                    Nuevo
                                                </a>
                                            </div>
                                            <div class="card-body">
                                                <table class="table table-bordered" id="dataTable1" width="100%"
                                                    cellspacing="0">
                                                    <thead>
                                                        <tr>
                                                            <th>N°</th>
                                                            <th>Vehiculo</th>
                                                            <th>Tarea</th>
                                                        </tr>
                                                    </thead>
                                                    <tfoot>
                                                        <tr>
                                                            <th>N°</th>
                                                            <th>Vehiculo</th>
                                                            <th>Tarea</th>
                                                        </tr>
                                                    </tfoot>
                                                    <tbody>
                                                        {{#each days}}
                                                        <tr>
                                                            <td style="vertical-align:middle;">{{math @index "+" 1}}
                                                            </td>
                                                            <td style="vertical-align:middle;"> {{CDia}}</td>
                                                            <td style="vertical-align:middle;">
                                                                <button type="button" class="btn btn-danger"
                                                                    data-toggle="modal" data-target="#alert"
                                                                    data-id="/start/delete/{{idCDia}}">
                                                                    <i class="fas fa-fw fa-trash-alt"></i>
                                                                </button>
                                                            </td>
                                                        </tr>
                                                        {{/each}}
                                                    </tbody>
                                                </table>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="row p-2">
                                    <div class="col-md-12">
                                        <div class="card">
                                            <div class="card-header">
                                                <i class="fas fa-table"></i>
                                                Personal que Concurrió
                                                <a class="btn btn-success float-right" data-toggle="modal"
                                                    data-target="#new" href="#">
                                                    Nuevo
                                                </a>
                                            </div>
                                            <div class="card-body">
                                                <table class="table table-bordered" id="dataTable" width="100%"
                                                    cellspacing="0">
                                                    <thead>
                                                        <tr>
                                                            <th>N°</th>
                                                            <th>Personal</th>
                                                            <th>Tarea</th>
                                                        </tr>
                                                    </thead>
                                                    <tfoot>
                                                        <tr>
                                                            <th>N°</th>
                                                            <th>Personal</th>
                                                            <th>Tarea</th>
                                                        </tr>
                                                    </tfoot>
                                                    <tbody>
                                                        {{#each days}}
                                                        <tr>
                                                            <td style="vertical-align:middle;">{{math @index "+" 1}}
                                                            </td>
                                                            <td style="vertical-align:middle;"> {{CDia}}</td>
                                                            <td style="vertical-align:middle;">
                                                                <button type="button" class="btn btn-danger"
                                                                    data-toggle="modal" data-target="#alert"
                                                                    data-id="/start/delete/{{idCDia}}">
                                                                    <i class="fas fa-fw fa-trash-alt"></i>
                                                                </button>
                                                            </td>
                                                        </tr>
                                                        {{/each}}
                                                    </tbody>
                                                </table>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="row p-2">
                                    <div class="col-md-12">
                                        <div class="card">
                                            <div class="card-footer">
                                                <button class="btn btn-primary">
                                                    Guardar
                                                </button>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <!--Forma de aviso -->
                        <div class="tab-pane fade" id="aviso" role="tabpanel" aria-labelledby="aviso-tab">
                            <div class="border container">
                                <div class="row p-2">
                                    <div class="col-md-8">
                                        <div class="card">
                                            <div class="card-header">
                                                <h5>Forma de Aviso</h5>
                                            </div>
                                            <div class="d-flex justify-content-center">
                                                <div class="btn" data-toggle="buttons">
                                                    {{#each notice}}
                                                    <label class="btn btn-light">
                                                        <input type="radio" name="idFAviso" id="option1"
                                                            value="{{idFAviso}}">
                                                        {{FAviso}}
                                                    </label>
                                                    {{/each}}
                                                </div>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="col-md-4">
                                        <div class="card">
                                            <div class="card-header">
                                                <h5>Hora salida Unidad(es)</h5>
                                            </div>
                                            <table>
                                                <thead>
                                                    <tr>
                                                        <th>Accion</th>
                                                        <th>Hora</th>
                                                    </tr>
                                                </thead>
                                                <tfoot>
                                                    <tr>
                                                        <th>Accion</th>
                                                        <th>Hora</th>
                                                    </tr>
                                                </tfoot>
                                                <tbody>
                                                    <tr>
                                                        <td>Aviso</td>
                                                        <td>
                                                            <input type="text" class="form-control" id="hora1"
                                                                name="HAviso" autocomplete="off"
                                                                placeholder="Seleccione una hora">
                                                            </input>
                                                        </td>
                                                    </tr>
                                                    <tr>
                                                        <td>Salida de Unidad(es)</td>
                                                        <td>
                                                            <input type="text" class="form-control" id="hora2"
                                                                name="HSalida" autocomplete="off"
                                                                placeholder="Seleccione una hora">
                                                            </input>
                                                        </td>
                                                    </tr>
                                                    <tr>
                                                        <td>Unidad(es) en el lugar</td>
                                                        <td>
                                                            <input type="text" class="form-control" id="hora3"
                                                                name="HLugar" autocomplete="off"
                                                                placeholder="Seleccione una hora">
                                                            </input>
                                                        </td>
                                                    </tr>
                                                    <tr>
                                                        <td>Fin de Op. y Ret.</td>
                                                        <td>
                                                            <input type="text" class="form-control" id="hora4"
                                                                name="HFin" autocomplete="off"
                                                                placeholder="Seleccione una hora">
                                                            </input>
                                                        </td>
                                                    </tr>
                                                    <tr>
                                                        <td>Ingreso al Cuartel</td>
                                                        <td>
                                                            <input type="text" class="form-control" id="hora5"
                                                                name="HCuartel" autocomplete="off"
                                                                placeholder="Seleccione una hora">
                                                            </input>
                                                        </td>
                                                    </tr>
                                                </tbody>
                                            </table>
                                        </div>
                                    </div>
                                </div>
                                <div class="row p-2">
                                    <div class="col-md-12">
                                        <div class="card">
                                            <div class="card-footer">
                                                <button class="btn btn-primary">
                                                    Guardar
                                                </button>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <!--Localizacion-->
                        <div class="tab-pane fade" id="localizacion" role="tabpanel" aria-labelledby="localizacion-tab">
                            <div class="border container">
                                <div class="row p-2">
                                    <div class="col-md-12">
                                        <div class="card">
                                            <div class="card-header">
                                                <h5>Localizacion</h5>
                                            </div>
                                            <div class="card-body">
                                                <div class="row container">
                                                    <div class="form-group col-md-4">
                                                        <label>Provincia: </label>
                                                        <select name="idProvincia" id="Provincia"
                                                            class="btn btn-light col-md-6">
                                                            {{#each provinces}}
                                                            <option value="{{idProvincia}}">{{Provincia}}</option>
                                                            {{/each}}
                                                        </select>
                                                    </div>
                                                    <div class="form-group col-md-4">
                                                        <label>Cantón: </label>
                                                        <select name="idCanton" id="Canton"
                                                            class="btn btn-light col-md-6">
                                                            <option>Seleccionar</option>
                                                        </select>
                                                    </div>
                                                    <div class="form-group col-md-4">
                                                        <label>Parroquia: </label>
                                                        <select name="idParroquia" id="Parroquia"
                                                            class="btn btn-light col-md-6">
                                                            <option>Seleccionar</option>
                                                        </select>
                                                    </div>
                                                </div>
                                                <div class="row container">
                                                    <div class="form-group col-md-6">
                                                        <label>Dirección </label>
                                                        <input class="form-control" type="text" name="Direccion"
                                                            placeholder="Ingrese dirección de la emergéncia" autofocus>
                                                    </div>
                                                    <div class="form-group col-md-4">
                                                        <label for="Provincia">Teléfono </label>
                                                        <input class="form-control" type="text" name="Telefono"
                                                            placeholder="Ingrese teléfono" autofocus>
                                                    </div>
                                                </div>
                                                <div class="row container">
                                                    <div class="form-group col-md-6">
                                                        <label>Sector N°:</label>
                                                        <input class="form-control" type="text" name="Sector"
                                                            placeholder="Ingrese sector" autofocus>
                                                    </div>
                                                    <div class="form-group col-md-4">
                                                        <label>Tipo de secctor:</label>
                                                        <div>
                                                            <label class="btn btn-light">
                                                                <input type="radio" name="TSector" id="option1"
                                                                    value="Urbano">
                                                                Urbano
                                                            </label>
                                                            <label class="btn btn-light">
                                                                <input type="radio" name="TSector" id="option1"
                                                                    value="Rural">
                                                                Rural
                                                            </label>
                                                        </div>
                                                    </div>
                                                    <div class="form-group col-md-12">
                                                        <label>Otros Datos:</label>
                                                        <textarea class="form-control" type="text" name="Otros"
                                                            rows="3"></textarea>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="row p-2">
                                    <div class="col-md-12">
                                        <div class="card">
                                            <div class="card-footer">
                                                <button class="btn btn-primary">
                                                    Guardar
                                                </button>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <!--Tipo de local-->
                        <div class="tab-pane fade" id="local" role="tabpanel" aria-labelledby="local-tab">
                            <div class="border container">
                                <div class="row p-2">
